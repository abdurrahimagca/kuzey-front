<%- include('./partials/_header', { header: gallery.header }) %> <%
gallery.sections.forEach(function(section) { %>
<section class="work-inners section-padding">
  <div class="container">
    <div class="section-title mb-70">
      <h2 class="text-uppercase"><%= section.title %></h2>
    </div>

    <div
      class="w-100 dsn-item-filter dsn-isotope dsn-masonry d-grid grid-md-2"
      data-dsn-item=".work-item"
      data-dsn-isotope='{"space": 30 }'
    >
      <% section.items.forEach(function(item) { %> <%-
      include('./partials/gallery-item', {item: item}); %> <% }); %>
    </div>
  </div>
</section>
<% }); %>
